(DEFUN F (X) (PLUS X Y))
(DEFUN G (Y) (F 10) )
(DEFUN H (Y) (F 20) )
(G 5)
(H 5)
(G (H 5))

(DEFUN DIFF (X Y) (COND ((EQ X Y) NIL) (T T)))
(DIFF 5 6)
(DEFUN MEM (X LIST) (COND ( (NULL LIST) NIL ) ( T (COND ( (EQ X (CAR LIST))T ) ( T (MEM X (CDR LIST)))))))
(MEM 3 (QUOTE (2 3 4)))

(DEFUN SQUARE (X) (TIMES X X))
(DEFUN MOD (X Y) (COND((LESS X Y) X)(T (MOD (MINUS X Y) Y))))
(SQUARE 10)
(MOD 15 3)